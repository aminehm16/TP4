#include <stdio.h>

int main() {
    int a, n;
    
    printf("Entrez deux entiers (a, n) : ");
    scanf("%d %d", &a, &n);

    printf("Entiers b congruents Ã  %d (mod %d) dans l'intervalle [0, %d] : ", a, n, n - 1);

    for (int b = 0; b < n; b++) {
        if (a % n == b) {
            printf("%d ", b);
        }
    }

    printf("\n");

    return 0;
}

